<div id="footer">
  <div class="container">
    <div class="row">
      <div class="col-md-12 text-center">
        <div class="footer_logo">
          <%= image_pack_tag "stewielogo.png", alt:"" %>
        </div>
      </div>
    </div>
  </div>
  <div class="container">
    <div class="row">
      <div class="col-md-12 text-center">
        <div class="social">
          <h2>Follow Me on Here</h2>
          <ul class="icon_list">
            <% @socials.each do |social| %>
              <li><a href=""><i class="<%= social.icon %>"></i></a></li>
            <% end %>
          </ul>
        </div>
      </div>
    </div>
  </div>
  <div class="container">
    <div class="row">
      <div class="col-md-12 text-center">
        <div class="copyright_text">
          <p>&copy; stewie 2001.All Right Reserved By <a href="https://www.facebook.com/nhoxnhut01/">Code Cafe Team</a></p>
          <p>Made with love for creative people.</p>
        </div>
      </div>
    </div>
  </div>
</div>